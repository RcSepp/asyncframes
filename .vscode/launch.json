	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"env": {
				"PYTHONPATH": "${workspaceFolder}"
			},
			"program": "${file}"
		},
		{
			"name": "unittest",
			"type": "python",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"module": "unittest",
			"args": [
				"test_asyncframes.Tests.test_Frame_current"
			]
		}
	]
}